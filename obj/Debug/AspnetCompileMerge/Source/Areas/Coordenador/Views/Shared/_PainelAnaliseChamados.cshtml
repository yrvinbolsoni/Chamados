
<script>
    function AtivaModal() {
        $('#ModelChamadoAnalise').modal('show')
    }

</script>





@*Painel  Analise Cordenador*@
<div class="card" style="max-width: 60rem;">
    <div class="card-header text-white bg-dark">
        Numeros de chamados no Sistema
    </div>
    <div class="card-body">
        <table>
            <tr>
                <td></td>
                <td></td>
                <td></td>
            </tr>
            <tbody>
                @foreach (var analise in ViewBag.ChamadoEmAnalise)
                {
                    <tr>
                        <td>
                            Em análise com <b>
                             @Ajax.ActionLink((string)analise.NomeUser, "ChamadosEmAnalisePorUsuario", new { id = analise.IdReponsavel } , new AjaxOptions(){
                               HttpMethod= "GET",
                               UpdateTargetId = "resultado",
                               InsertionMode = InsertionMode.Replace
                           }, new { @class= "text-dark", @onclick= "AtivaModal()" })
                            </b>
                        </td>
                        <td style="padding-left: 20px;"> @analise.Quantidade</td>
                        <td style="padding-left: 20px;"><span class="text-danger">@analise.DiasParado  Chamados parados a mais de 2 dias  </span>  </td>
                    </tr>

                }
            </tbody>
        </table>
    </div>
</div>


<div class="modal fade modal-ChamadoAnalise" tabindex="-1" role="dialog" aria-labelledby="ModelChamadoAnalise" id="ModelChamadoAnalise" aria-hidden="true">
    <div class="modal-dialog modal-xl" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal"><span aria-hidden="true">×</span><span class="sr-only">Close</span></button>
                <h3 class="modal-title" id="lineModalLabel">Detalhes</h3>
            </div>
            <div class="modal-body">
                <div id="resultado">


                </div>
                <hr />
            </div>
        </div>
    </div>
</div>
